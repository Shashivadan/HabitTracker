#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/shashi/Desktop/HabitTrackerAPI/node_modules/which/bin/node_modules:/home/shashi/Desktop/HabitTrackerAPI/node_modules/which/node_modules:/home/shashi/Desktop/HabitTrackerAPI/node_modules:/home/shashi/Desktop/node_modules:/home/shashi/node_modules:/home/node_modules:/node_modules:/home/shashi/Desktop/HabitTrackerAPI/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/shashi/Desktop/HabitTrackerAPI/node_modules/which/bin/node_modules:/home/shashi/Desktop/HabitTrackerAPI/node_modules/which/node_modules:/home/shashi/Desktop/HabitTrackerAPI/node_modules:/home/shashi/Desktop/node_modules:/home/shashi/node_modules:/home/node_modules:/node_modules:/home/shashi/Desktop/HabitTrackerAPI/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../which/bin/node-which" "$@"
else
  exec node  "$basedir/../which/bin/node-which" "$@"
fi
